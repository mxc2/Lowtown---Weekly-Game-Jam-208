if(counter &lt; string_length(text[page])){
    counter++;
    if(counter mod 4 == 0){
        audio_play_sound(voice, 10, false);
    }
}

var substr = string_copy(text[page], 1, counter);

c = text_col;
draw_text_ext_color(text_x, text_y, substr, text_height, text_max_width, c,c,c,c, 1);
