//Draw portrait
draw_sprite(portrait, portrait_index, port_x, port_y);

//Draw Portrait Frame
draw_sprite(frame, 1, port_x, port_y);

//draw namebox
draw_sprite(namebox, 0, namebox_x, namebox_y);


//TEXT
draw_set_font(font);
//Draw Name
var c = name_text_col;
draw_set_halign(fa_center); draw_set_valign(fa_middle);
