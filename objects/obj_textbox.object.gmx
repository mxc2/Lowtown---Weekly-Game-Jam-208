y_buffer = 8;

text_x = box_x + x_buffer;
text_y = box_y + y_buffer;
name_text_x = namebox_x + (namebox_width/2);
name_text_y = namebox_y + (namebox_height/2);
text_max_width = box_width - (2*x_buffer);

portrait_index = 0;
counter = 0;

text[0] = "";
page = 0;
name = "";
voice = typing;

interact_key = ord("E");

text_col = c_black;
